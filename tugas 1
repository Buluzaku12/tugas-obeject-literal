let studentsScore = [
    { name: 'Andi', score: 90 },
    { name: 'Rudi', score: 80 },
    { name: 'Dira', score: 100 },
];

let highestScoreStudent = studentsScore.reduce((max, student) => student.score > max.score ? student : max, studentsScore[0]);

console.log(`Nama: ${highestScoreStudent.name}, Nilai: ${highestScoreStudent.score}`);
